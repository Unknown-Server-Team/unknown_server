{
  "name": "unknown-server",
  "version": "2.6.0",
  "description": "A modern Express.js server with built-in managers and utilities - Full TypeScript Migration",
  "main": "server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "unknown": "./cli/cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "start:ts": "npm run build && node dist/server.js",
    "start:hybrid": "node -r ts-node/register server.ts",
    "dev": "nodemon --exec 'clear && node' server.js",
    "dev:ts": "nodemon --ext ts,js --exec 'npm run build && node dist/server.js'",
    "dev:hybrid": "nodemon --ext ts,js --exec 'node -r ts-node/register server.ts'",
    "cluster": "node cluster.js",
    "cluster:ts": "npm run build && node dist/cluster.js",
    "cluster:hybrid": "node -r ts-node/register cluster.ts",
    "start:pm2": "pm2 start ecosystem.config.js",
    "start:pm2:dev": "pm2 start ecosystem.config.js --env development",
    "start:pm2:prod": "pm2 start ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop unknown-server",
    "monitor": "pm2 monit",
    "cli": "node ./cli/cli.js",
    "build": "tsc",
    "build:clean": "rimraf dist && npm run build",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test:types": "tsc --noEmit --project tsconfig.json",
    "migrate:check": "echo 'TypeScript migration status: 16/49 files converted'",
    "postbuild": "echo 'TypeScript build completed. Hybrid compatibility maintained.'"
  },
  "keywords": [
    "express",
    "server",
    "websocket",
    "api",
    "unknown-server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^6.7.0",
    "figlet": "^1.7.0",
    "figures": "^3.2.0",
    "helmet": "^6.1.5",
    "inquirer": "^8.2.6",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.7",
    "mathjs": "^14.3.0",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.10.0",
    "open": "^8.4.2",
    "opossum": "^8.4.0",
    "ora": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "validator": "^13.12.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/figlet": "^1.7.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^6.4.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
